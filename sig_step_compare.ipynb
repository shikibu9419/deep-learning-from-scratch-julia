   "source": [
    "import Base: convert\n",
    "convert(::Type{Integer}, x::Bool) = x ? 1 : 0\n",
    "\n",
    "# dot: vectorize calucuration\n",
    "sigmoid(x) = 1 ./ (1 .+ exp.(-x))\n",
    "step(x) = convert.(Integer, x .> 0)\n",
    "relu(x) = max.(0, x)"
   ]
